-+Abstract.TimedObserver = Class.create(PeriodicalExecuter, {
-+  initialize: function($super, element, frequency, callback) {
-+    $super(callback, frequency);
-+    this.element   = $(element);
-+    this.lastValue = this.getValue();
-+  },
-+
