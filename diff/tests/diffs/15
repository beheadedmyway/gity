-+    },
-+    attr: function(m) {
-+      m[1] = m[1].toLowerCase();
-+      m[3] = m[5] || m[6];
-+      return new Template(Selector.xpath.operators[m[2]]).evaluate(m);
-+    },
-+    pseudo: function(m) {
